		<script type="text/javascript" charset="utf-8">
	var _cn = '__g_pre';
	var _cn_u = '__g_preu';
	var _cn_d = new Date().getTime();

	var _cnt = jQuery.cookie(_cn);
	if (_cnt == null) {
		_cnt = 0;
	}
	_cnt = _cnt % 4;

	var _cn_un = jQuery.cookie(_cn_u);
	if (_cn_un == null) {
		_cn_un = 0;
	}

	if(_cnt == 1){
		document.write('<img src="http://track.gawker.com/stats/count/post?i=AD:238&amp;s=55ba987f');
		if ( !_cn_un || _cn_un < _cn_d - 2592000000) {
			document.write('&amp;u=1');
			_cn_un = _cn_d;
		}
		document.write('" height="0" width="0" id="preroll_tracker" class="track" alt="track" />');
	}
	_cnt++;
	jQuery.cookie(_cn, _cnt , {path: '/'});
	jQuery.cookie(_cn_u, _cn_un , {path: '/', expires: 365});
</script>
