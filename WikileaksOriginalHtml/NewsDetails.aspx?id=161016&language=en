<script type="text/javascript">
	function Subscribe()
	{
		window.open('./NewsletterPage.aspx?type=In&language=en', null, 'height=220,width=520,top=120,left=250,menubar=0,resizable=0,scrollbars=1,status=0,titlebar=1,toolbar=0');
	}

	function UnSubscribe()
	{
		window.open('./NewsletterPage.aspx?type=Out&language=en', null, 'height=220,width=520,top=120,left=250,menubar=0,resizable=0,scrollbars=1,status=0,titlebar=1,toolbar=0');
	}
</script>
                                            
                                            
                                            
