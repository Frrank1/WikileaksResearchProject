        <tr>
            <td valign="top">
                <table width="200" cellpadding="2" align="left" cellspacing="0" border="0">
                    <tr id="ctl00_ContentPlaceHolder1_imgRow">
	<td>
                            <a href="javascript:void(0)" id="ctl00_ContentPlaceHolder1_lnkImgNews" onclick="window.open('PictureFrame.aspx?id=216059', null,'menubar=0,directories=1,resizable=1,scrollbars=1,statusbar=0,titlebar=0');">
                                <img src="/NewsSite/WebsiteImages/PicturesFolder/f2b3c61a-6d63-4f41-b452-09983bbb842b_top.jpg" id="ctl00_ContentPlaceHolder1_imgNews" border="0" /></a></td>
</tr>

                    <tr id="ctl00_ContentPlaceHolder1_imgCaptionRow">
	<td class="PictureCaption" style="padding-right: 5px; padding-left: 5px">
                            
                        </td>
