<script type="text/javascript">
	function PerformSearch()
	{
		search = document.getElementById("txtSearch");
		
		if (search.value.replace(" ", "") == "")
			return false;
		else
		{
			radio = document.getElementById("rdSearchWeb");
			if(radio.checked)
			{
				// Internet search
				window.open("http://www.google.com/search?hl=en&q=" + search.value);
			}
			else
			{
				//Site search				
				location.href = "ArticlesSearch.aspx?searchText=" + search.value + "&language=en";				
			}
		}
	}
	
	function changeLanguage(id)
	{
	    if (id == "ar")
	        location.href = "HomePage.aspx?language=" + id;
	    else
	        location.href = "News.aspx?language=" + id;
	}
	
	function OpenSearch()
	{
	    location.href = "ArticlesSearch.aspx?language=en";
	}
</script>
