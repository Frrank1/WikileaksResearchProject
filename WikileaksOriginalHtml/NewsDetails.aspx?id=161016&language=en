<script type="text/javascript">
	function loadRegionPage( id )
	{
		var ddlRegions = document.getElementById( id );
		location.href='GeoCategory.aspx?id='+ ddlRegions.options[ddlRegions.selectedIndex].value + '&language=en';
	}
