    <script type="text/javascript" language="javascript">
    function ValidateForm()
	{
		var objTxtEmail = document.getElementById("ctl00_ContentPlaceHolder1_txtEmail");
		var objTxtName = document.getElementById("ctl00_ContentPlaceHolder1_txtName");
		var objTxtCountry = document.getElementById("ctl00_ContentPlaceHolder1_txtCountry");
		var objTxtFeedBackTitle = document.getElementById("ctl00_ContentPlaceHolder1_txtFeedBackTitle");
		var objSpanEmail = document.getElementById("spanEmail");
		var objSpanName = document.getElementById("spanName");
		var objSpanCountry = document.getElementById("spanCountry");
		var objSpanFeedBackTitle = document.getElementById("spanTitle");
		objSpanEmail.innerHTML = "";
		objSpanName.innerHTML = "";
		objSpanCountry.innerHTML = "";
		objSpanFeedBackTitle.innerHTML = "";
		
		if(objTxtName.value == "")
		{
			//username was not supplied
			//
			objSpanName.innerHTML = "Please provide your name";
			objTxtName.focus();
			return false;
		}
			
		
		
		if(objTxtEmail.value == "")
		{
			// Email was not supplied
			//
			objSpanEmail.innerHTML = "Please provide an Email!!";
			objTxtEmail.focus();
			return false;
		}	
		
		
				
		if(objTxtCountry.value == "")
		{
			//Country was not selected
			//
			objSpanCountry.innerHTML = "Please Insert a Country.";
			objTxtCountry.focus();
			return false;
		}
		
		if(objTxtFeedBackTitle.value == "")
		{
			// Comments was not supplied
			//
			objSpanFeedBackTitle.innerHTML = "Please write a comments title!!";	
			objTxtFeedBackTitle.focus();
			return false;
		}	
	}
    	
    function showKeyboard()
    {
	    var tableAll = document.getElementById("tableKeyboard");
	    var tdShow = document.getElementById("tdKeyboardShow");
	    var form = document.getElementById("Form1");
    				
	    tableAll.style.display = "block";
	    tdShow.style.display = "none";
    }

    function hideKeyboard()
    {
	    var tableAll = document.getElementById("tableKeyboard");
	    var tdShow = document.getElementById("tdKeyboardShow");
    				
	    tableAll.style.display = "none";
	    tdShow.style.display = "block";
    }

	function charCount(message,text)
	{
		var messageObj = document.getElementById("ctl00_ContentPlaceHolder1_txtFeedBackDetails");
		var wordCount = document.getElementById("ctl00_ContentPlaceHolder1_wordCount");
		var charBannObj = document.getElementById("ctl00_ContentPlaceHolder1_charBann");
		var longitude = wordCount.value - messageObj.value.length;
		if(longitude <= 0)
		{
			longitude = 0;
			text = '<span> ' + text + ' </span>';
			messageObj.value = messageObj.value.substr(0, wordCount.value);
		}
		charBannObj.innerHTML = text.replace("{CHAR}", longitude) + " / " + wordCount.value;
	}
    </script>


                                                    </td>
                                                </tr>
                                            </table>
                                        </td>
                                        <td style="width: 10px"></td>
                                        <td style="width: 320px" valign="top" align="left">
                                            

