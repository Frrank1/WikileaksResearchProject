    var Validate = function(XMLHttpRequest) {        
        var hasNoErrors = true;
        var isPosted = false;
        var keys = new Array("signatureErrors", "textErrors", "acceptDisclaimerErrors");        
        $(".feedback").hide();
        $.each(keys, function(key, value) {
            if (XMLHttpRequest.getResponseHeader(value) != null && XMLHttpRequest.getResponseHeader(value).length > 0) {
                $("#" + value).css("display", "block");
                hasNoErrors = false;                
                $("#" + value).parent().children(":input").addClass("user-input-error");
            }
            else {
                $("#" + value).css("display", "none");
                $("#" + value).parent().children(":input").removeClass("user-input-error");
