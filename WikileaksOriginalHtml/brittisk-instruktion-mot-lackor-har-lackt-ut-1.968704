<script type="text/javascript">
  var friendActivities = {
    displayedActivities : 3,
    currentActivityOffset : 0,
    loginInfo : null,

    setFacebookLoggedIn : function(self, p) {
      var o = self;
      
      o.clearFacebookEventList();
      
      if (p.isLoggedIn && p.facebookUserName != null) {
        o.loginInfo = p;
        o.loadPosts();
      }
      else {
      }
    },
    loadPosts : function() {
         var o = this;
         var friendQueryText = "select uid, name, pic_small from user " +
                               "where uid in (select uid2 from friend where uid1 = {0}) or uid = {0}";
                                 
         var streamQueryText = "select source_id, message, attachment, app_data, app_id, created_time from stream"
                               + " where (source_id in (select uid2 from friend where uid1={0}))"
                               + " and app_id = {1}"
                               + " and attachment != ''"
                               + " order by created_time desc"
                               + " limit {2} offset {3}"
                               ;
           
         var friendQuery = FB.Data.query(friendQueryText, o.loginInfo.userId);
         var streamQuery = FB.Data.query(streamQueryText, o.loginInfo.userId, o.loginInfo.facebookAppId
         , o.displayedActivities + 1, o.currentActivityOffset
         );
         FB.Data.waitOn([streamQuery, friendQuery], function(args) {
            
            if(streamQuery.value.length > o.displayedActivities) {
                $(".show-more-button").show();
            }
            else {
                $(".show-more-button").hide();
            }
            
            for(var i = 0; i < Math.min(streamQuery.value.length, o.displayedActivities); i++) {
                var post = streamQuery.value[i];
                
                var user = o.getUserById(friendQuery.value, post.source_id);
                
                if(user != null) {
                    var app_data = 
                        (post.app_data != null && post.app_data.attachment_data != null)
                        ? $.parseJSON(post.app_data.attachment_data,true)
                        : null;
                    
                    post.wallMsg = app_data != null && app_data.wallMsg != null
                            ? app_data.wallMsg
                            : user.name+': ';

                    o.displayFacebookEvent(post, user);
                }
            }
         });
    },
    
    getUserById : function(users, uid) {
        for(var i = 0; i < users.length; i++) {
            if(users[i].uid == uid) {
                return users[i];
            }
        }
        
        return null;
    },
    
    displayFacebookEvent : function(post, user) {
        $(".facebook-eventslist").append('<li class="clear">' +
          '<div class="profile-image">' +
            '<a href="http://www.facebook.com/profile.php?id=' + user.uid + '" class="external-url"><img src="' + (user.pic_small == null ? 'http://static.ak.connect.facebook.com/pics/q_silhouette_logo.gif' : user.pic_small) + '" class="user-profilepicture" /></a>' +
          '</div>' +
          '<p>' + post.wallMsg + ' <a href="' + post.attachment.href + '">' + post.attachment.name + '</a>. ' +
          '<span class="time">' + this.getPostCreationTime(post) + '</span>' +
          '</p></li>'
        );
    },
    
    clearFacebookEventList : function()
    {
        $(".facebook-eventslist").html("");
        $(".show-more-button").hide();
    },
    
    getPostCreationTime : function(post) {
        var d = new Date();
        d.setTime(post.created_time * 1000);
        return d.toLocaleDateString();
    }
  };
</script>
