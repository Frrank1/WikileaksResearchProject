        return false;
    }

    function getCookie(cname){
        var C = document.cookie.split(';'), c;
        while(c = C.pop()) {
            c = c.replace(/^\s*/, '');
            if (c.indexOf(cname+'=') == 0){
                return c.substring(cname.length + 1, c.length);
            }
        }
        return false;
    }

