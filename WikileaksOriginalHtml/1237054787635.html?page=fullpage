<!--
function getQueryVariable(variable) {
  var query = window.location.search.substring(1);
  var vars = query.split("&");
  for (var i=0;i<vars.length;i++) {
    var pair = vars[i].split("=");
    if (pair[0] == variable) {
      return pair[1];
    }
  } 
  return ''; 
}
var totalpagespagination = 2;
var currpagepagination = 1;
var tcurrpage = getQueryVariable('page');
var fullpage = false;
if(tcurrpage == "fullpage") {
	fullpage=true;
}
var navstrpagination = "";
var detailsstrpagination = "";
if( fullpage == false ) {
	if(tcurrpage != '') {
		var icurrpage = Number(tcurrpage);
		if(icurrpage > 0) {
			currpagepagination = icurrpage;
		}
	}
	if(currpagepagination > totalpagespagination) {
		currpagepagination = totalpagespagination;
	}//New Fix
	var prevpagenum = currpagepagination - 1;
	var nextpagenum = currpagepagination + 1;
	var selfpath = window.location.pathname;
	navstrpagination = "<ul class=\"pages\">";
	if(currpagepagination > 1) {
		if(prevpagenum == 1) {
			navstrpagination += "<li class=\"previous\"><a href=\"" + selfpath + "\" rel=\"nofollow\">Previous page</a></li>";
		}
		else {
			navstrpagination += "<li class=\"previous\"><a href=\""+ selfpath + "?page=" + prevpagenum + "\" rel=\"nofollow\">Previous page</a></li>";
		}
	}
	navstrpagination += "<li class=\"pagecount\">Page ";
	for (var i=1; i<=totalpagespagination; i++) {
		if(currpagepagination == i) {
			navstrpagination += " <em>"+i+"</em>";
		}
		else {
			if(i == 1) {
				navstrpagination += " <a href=\"" + selfpath + "\" rel=\"nofollow\">"+i+"</a>";
			}
			else {
				navstrpagination += " <a href=\"" + selfpath + "?page=" + i + "\" rel=\"nofollow\">"+i+"</a>";
			}
		}
	}
	navstrpagination += "</li>";
	if(currpagepagination < totalpagespagination) {
		navstrpagination += "<li class=\"next\"><a href=\"" + selfpath + "?page=" + nextpagenum + "\" rel=\"nofollow\">Next page</a></li>";
		if( totalpagespagination > 2 ) {
			navstrpagination += "<li class=\"full-article\"><a href=\"" + selfpath + "?page=fullpage" + "#contentSwap" + nextpagenum + "\" rel=\"nofollow\">View full article</a></li>";
		}
	}
	navstrpagination += "</ul>";
	var detailsnavstr = "";
	if( (currpagepagination < totalpagespagination) && (totalpagespagination > 1) ) {
		detailsnavstr = " | <a href=\"" + selfpath + "?page=fullpage" + "#contentSwap" + currpagepagination + "\" rel=\"nofollow\">Single page</a>";
	}
	detailsstrpagination = "<span class=\"pagecount\">Page " + currpagepagination + " of " + totalpagespagination + detailsnavstr + "</span>";
}
