						$("#messagebox").focus();

   			if ( $("#messagebox").length > 0 )
   			{
	   			if($('#messagebox').val().length == 0)
   				{
   					$('#charcount').html('0');
   				}
				else
				{
					$('#charcount').html($('#messagebox').val().length);
   				}
   			}

   			$('#messagebox').keyup(function() {
 	 			var charLength = $(this).val().length;
 	 			if(charLength == '0')
 	 			{
 	 				$('#charcount').html('0');
 	 			}
 	 			else
 	 			{
  					$('#charcount').html(charLength);
  				}
				});

			$('#oauthlink').mouseenter(function(){
				$('#light').fadeOut(200);
				$('#dark').fadeIn(200);
				});

			$('#oauthlink').mouseleave(function(){
				$('#dark').fadeOut(200);
				$('#light').fadeIn(200);
				});
			
			$('#topsyinput').focus(function(){
				if ( $(this).val() == 'Search Twitter')
				{
					$(this).val('');	
				}
				});
			
			$('#topsyinput').blur(function(){
				if ( $(this).val() == '')
				{
					$(this).val('Search Twitter');	
				}
				});
			
			});
