<script type="text/javascript">
function blogPostCommentRequest() {
	var requestBatch = new RequestBatch();
	requestBatch.AddToRequest(pluckUtils.makeCommentPageRequestObject(articleKey));
 	requestBatch.BeginRequest(serverUrl, getComments);
}
//uses blog post comment info

function getComments(responseBatch) {
	if (responseBatch.Responses.length != 0) {

		/* get number of Pluck comments */
		if (responseBatch.Responses[0].CommentPage.NumberOfComments != "undefined") {
			var contentDivs = document.getElementById("contentContainer").getElementsByTagName("li");
			var commentString = (responseBatch.Responses[0].CommentPage.NumberOfComments > 0) ? "Comments (" + responseBatch.Responses[0].CommentPage.NumberOfComments + ")" : "Comments";
			for (var i=0; i<contentDivs.length; i++) {
				if (contentDivs[i].className == "shareComments") {
					contentDivs[i].innerHTML = '<a title="Go to Comments Section" href="#comments">' + commentString + '</a>';
				}
			}
		}
		/* end get number of Pluck comments */
	
		for (var i=0; i<3; i++) {
                
            			if (responseBatch.Responses[i].CommentPage != null) {
						responseBatch.Responses[0] = responseBatch.Responses[i];
						break;
            			}        
		}
		pluckBlogComments.showBlogPostComments(responseBatch);
	}

}

//initiates call to Pluck
blogPostCommentRequest();
</script>




<!--END: M005 Render Pluck Comments -->

























	  
	  
	


	
  			  

