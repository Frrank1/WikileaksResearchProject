<script type="text/javascript">
	/* get page URL without query string */
	function getFilteredURL() {
		url = window.location.href;
		if (url.indexOf('?') != -1) {
			/* there's a query string */
			pos = url.indexOf('?');
			url = url.substr(0, pos);
		}

		return url + "?linkedfrom=fb";
	}
</script>

