<script type="text/javascript" langauage="javascript">
function setSelectedSearchType(val)
{
	var searchType=document.getElementById('search_type');
	var search_type_list = document.getElementById('searchTypes');
	searchType.value=val;
	switch(val) {
		case "news":
			search_type_list.getElementsByTagName('li')[0].innerHTML ='|<strong class="selectedTab">News</strong>|';
			search_type_list.getElementsByTagName('li')[1].innerHTML ='<a href="javascript:setSelectedSearchType(\'web\')">Web</a> |';
			break;
		case "web":
			search_type_list.getElementsByTagName('li')[1].innerHTML ='<strong class="selectedTab">Web</strong> |';
			search_type_list.getElementsByTagName('li')[0].innerHTML = '| <a href="javascript:setSelectedSearchType(\'news\')">News</a> |';
			break;
	}
}

</script>


