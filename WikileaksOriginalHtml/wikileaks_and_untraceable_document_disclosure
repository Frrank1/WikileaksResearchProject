    <script language="javascript" type="text/javascript">
tinyMCE.init({
	
  mode : "exact",
  theme : "advanced",
  plugins : "",
  browsers : "msie,gecko",
  cleanup : false,
  content_css : "http://media.nowpublic.com/css/modules/np_tinymce/content.css",
  theme_advanced_toolbar_location : "top",
  theme_advanced_statusbar_location : "bottom",
  theme_advanced_styles : "Body=tinyMCE-body;Headline=tinyMCE-headline;Subhead=tinyMCE-subhead;",
  theme_advanced_resizing : true,
  theme_advanced_resize_horizontal : false,theme_advanced_buttons1 : "bold,italic,separator,link,unlink,separator,bullist,numlist,separator,code,separator,indent,outdent,separator,charmap,separator,styleselect",
    theme_advanced_buttons2 : "",
    theme_advanced_buttons3 : "",
	elements : "edit-comment"
});

  function mceToggle(id, linkid) {
    element = document.getElementById(id);
    link = document.getElementById(linkid);

    if (tinyMCE.getEditorId(id) == null) {
      tinyMCE.addMCEControl(element, id);
      element.togg = 'on';
      link.innerHTML = 'Disable HTML Editor / View Source';
      link.href = "javascript:mceToggle('" +id+ "', '" +linkid+ "');";
      link.blur();
    }
    else {
      tinyMCE.removeMCEControl(tinyMCE.getEditorId(id));
      element.togg = 'off';
      link.innerHTML = 'Enable HTML Editor';
      link.href = "javascript:mceToggle('" +id+ "', '" +linkid+ "');";
      link.blur();
    }
  }
  
  
  function np_tinymce_init() {
    if (!isOpera && ((isWin && isMinIE5) || isMoz)) {
    var a;
                            var element;
                            var d;
                             
                                if (element = document.getElementById('edit-comment')) {
                                  a = document.createElement('a');
                                  d = document.createElement('div');
                                  d.className = 'tinymce-control-links';
                                  a.href = 'javascript:mceToggle("edit-comment", "link-edit-comment")';
                                  a.appendChild(document.createTextNode('Disable HTML Editor / View Source'));
                                  a.id = 'link-edit-comment';
                                  d.appendChild(a);
                                  element.parentNode.appendChild(d);
                                }
                                
    }
  }
  
</script>
